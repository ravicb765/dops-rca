{
  "name": "gitops-rca",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@backstage/app-defaults": "^1.5.0",
    "@backstage/backend-common": "^0.22.0",
    "@backstage/backend-plugin-api": "^0.6.14",
    "@backstage/catalog-model": "^1.4.5",
    "@backstage/config": "^1.2.0",
    "@backstage/errors": "^1.2.4",
    "@backstage/plugin-auth-backend": "^0.22.0",
    "@backstage/plugin-auth-node": "^0.4.15",
    "@backstage/plugin-catalog-backend": "^1.21.0",
    "@backstage/plugin-kubernetes": "^0.12.0",
    "@backstage/plugin-kubernetes-backend": "^0.17.0",
    "@backstage/plugin-permission-backend": "^0.5.0",
    "@backstage/plugin-permission-node": "^0.7.0",
    "@backstage/plugin-search-backend": "^1.5.0",
    "@backstage/plugin-techdocs-backend": "^1.9.0",
    "@langchain/community": "^0.0.25",
    "@langchain/openai": "^0.0.15",
    "@node-saml/passport-saml": "^4.0.2",
    "langchain": "^0.1.20",
    "lokijs": "^1.5.12",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@backstage/cli": "^0.26.0",
    "@types/node": "^18.19.0",
    "concurrently": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn start-backend\"",
    "start": "yarn workspace app start",
    "start-backend": "yarn workspace backend start",
    "build": "backstage-cli repo build",
    "build:backend": "backstage-cli backend:build",
    "build:frontend": "backstage-cli app:build",
    "lint": "backstage-cli repo lint",
    "test": "backstage-cli repo test",
    "k8sgpt:deploy": "helm upgrade --install k8sgpt k8sgpt/k8sgpt --namespace k8sgpt --create-namespace -f ./k8sgpt-values.yaml",
    "loadtest": "artillery run ./loadtest-scenario.yml"
  },
  "packageManager": "yarn@4.4.1"
}
